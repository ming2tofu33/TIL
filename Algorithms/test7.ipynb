   "source": [
    "letters = ['a', 'b', 'c']\n",
    "\n",
    "for i in range(1 << len(letters)):\n",
    "    selected = []\n",
    "    for j in range(len(letters)):\n",
    "        if i & (1 << j):\n",
    "            selected.append(letters[j])\n",
    "\n",
    "    print(selected)"
   ]
  }
